{
  "name": "jalter",
  "version": "3.0.2",
  "description": "A NodeJS wrapper for Discord Selfbot",
  "main": "build/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rm -rf build/ && tsc",
    "docs": "npx typedoc --entryPointStrategy expand src/ index.ts",
