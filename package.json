    "api",
    "discordapp"
  ],
  "author": "sinkaroid",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sinkaroid/jalter/issues"
  },
  "homepage": "https://github.com/sinkaroid/jalter#readme",
  "dependencies": {
    "phin": "^3.6.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "changelog": "^1.4.2",
    "eslint": "^7.32.0",
    "npx": "^10.2.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.5",
    "typedoc-plugin-rename-defaults": "^0.4.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
